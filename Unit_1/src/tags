Bool	myDefine.c	/^typedef int Bool;$/;"	t	file:
ERROR	myDefine.c	5;"	d	file:
FALSE	myDefine.c	3;"	d	file:
INFEASIBLE	myDefine.c	6;"	d	file:
OK	myDefine.c	4;"	d	file:
OVERFLOW	myDefine.c	7;"	d	file:
Status	myDefine.c	/^typedef int Status;$/;"	t	file:
TRUE	myDefine.c	2;"	d	file:
